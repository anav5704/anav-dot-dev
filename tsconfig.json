{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "strictNullChecks": true,
        "allowJs": true,
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@lib/*": [
                "lib/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ]
        }
    }
}